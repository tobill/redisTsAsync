{
    "name": "redis-ts-promise",
    "version": "0.1.0",
    "description": "redis lib for typescript with promises fopr async / await use",
    "repository": "",
    "author": "Tobias Billep <t.billep@googlemail.com>",
    "license": "MIT",
    "scripts": {
        "start": "node dist/server.js",
        "test": "node runTests.js"
    },
    "dependencies": {
        "redis": "^2.7.1"
    },

    "devDependencies": {
        "@types/assert": "0.0.31",
        "@types/bluebird": "^3.5.8",
        "@types/express": "^4.0.36",
        "@types/jest": "^20.0.2",
        "@types/redis": "^2.6.0",
        "jest": "^20.0.4",
        "ts-jest": "^20.0.6",
        "typescript": "^2.4.1"
    },

    "jest": {
        "globals": {
            "__TS_CONFIG__": "tsconfig.json"
        },
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
        },
        "testMatch": [
            "**/__tests__/**/*.spec.(ts|js)"
        ],
        "testEnvironment": "node"
    }
}